<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.marshallbros.windows.GeometryMenuController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="800.0" pickOnBounds="true">
         <image>
            <Image url="@../resources/geometryimg.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="190.0" layoutY="50.0" prefHeight="220.0" prefWidth="420.0" style="-fx-background-color: #640DA9;">
         <children>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="400.0" spacing="3.0" style="-fx-background-color: #5868D7;">
               <children>
                  <Label text="Select your Categoory: " textFill="WHITE">
                     <font>
                        <Font name="Arial" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox prefWidth="150.0" />
